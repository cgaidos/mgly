                <div class="guest-card">
                    <div class="box light mt-sm-10">


                        <div class="row middle-row-lg text-center-md">

                            <div class="col-lg-4 col-20 middle-col">
                                <div class="avatar border">
                                    <img class="px-150" src="/moowgly/img/dev/avatar.png" alt="Michael Keller">
                                </div>
                            </div>

                            <div class="col-lg-8 col-20 middle-col mt-md-15">
                                <h4 class="user-name dark mt-5">Chinese lessons</h4>
                                <h5 class="dark mt-5 display-block">Michael Keller</h5>
                                <h5 class="user-address dark">Munich, Germany <i class="icon-marker"></i></h5>

                                <div class="mt-15">
                                    {{ rating.star(3) }}
                                </div>
                                <div class="mt-5">
                                    {{ rating.ball(4) }} <span style="margin-left: 15px" class="text-grey display-block-md text-italic">(rating of moowhly users)</span>
                                </div>
                            </div>

                        </div>

                        <div class="row">
                            <div class="col-md-12 col-30">
                                <div class="separator-horizontal"></div>
                            </div>
                        </div>

                        <div class="row">

                            <div class="col-md-12 col-20">
                                <h3 class="mb-20">Chinese lessons</h3>
                                <p>I need someone who can teach my two children chinese.</p>
                                <p>Wednesday afternoon from 16:00 to 18:00.</p>
                                <p>I offer 20€ per hour.</p>
                            </div>
                        </div>
                    </div>

                    <div class="box light mt-sm-10 mt-10">
                        <h3>Reviews</h3>

                        <div class="reviews">
                            {% include './guest/reviews/partials/review-host.twig' %}
                            {% include './guest/reviews/partials/review-host.twig' with {answer: true} %}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
